{
    "env": {
        "commonjs": true,
        "es2020": true,
        "node": true
    },
    "plugins": [],
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
        "indent": [ "error", 2 ],
        "semi": "error",
        "no-console": "off",
        "quotes": [ "error", "single" ]
    },
    "root": true
}
